{
	"name": "ImageTemplate",
	"visualStyles": {
		"image": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"background": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"transparency": 0
				}],
				"border": [{
					"color": { "solid": { "color": "#808080"}},
					"show": true,
					"radius": 0
				}],
				"title": [{
					"show": true,
					"fontColor": { "solid": { "color": "#000000" } },
					"background": { "solid": { "color": "#FFFFFF" } },
					"alignment": "center"
				}],
				"imageScaling": [{
					"imageScalingType": "Normal"
				}]
			}
		}
	}
}