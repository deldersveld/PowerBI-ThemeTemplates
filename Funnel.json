{
	"name": "FunnelTemplate",
	"visualStyles": {
		"funnel": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"background": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"transparency": 0
				}],
				"border": [{
					"color": { "solid": { "color": "#808080"}},
					"show": true,
					"radius": 0
				}],
				"title": [{
					"show": true,
					"fontColor": { "solid": { "color": "#000000" } },
					"background": { "solid": { "color": "#FFFFFF" } },
					"alignment": "center"
				}],
				"categoryAxis": [{
					"show": true,
					"color": { "solid": { "color": "#000000"}},
					"fontSize": 10,
					"fontFamily": "Verdana"
				}],
				"dataPoint": [{
					"defaultColor": { "solid": { "color": "#010000"}},
					"showAllDataPoints": true
				}],
				"labels": [{
					"show": true,
					"color": { "solid": { "color": "#01B8AA"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"labelPosition": "OutsideEnd",
					"fontSize": 10,
					"fontFamily": "Verdana"
				}],
				"percentBarLabel": [{
					"show": true,
					"color": { "solid": { "color": "#01B8AA"}},
					"fontSize": 10,
					"fontFamily": "Verdana"
				}]
			}
		}
	}
}