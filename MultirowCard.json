{
	"name": "MultiRowCardTemplate",
	"visualStyles": {
		"multiRowCard": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"background": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"transparency": 0
				}],
				"border": [{
					"color": { "solid": { "color": "#808080"}},
					"show": true,
					"radius": 0
				}],
				"title": [{
					"show": true,
					"fontColor": { "solid": { "color": "#000000" } },
					"background": { "solid": { "color": "#FFFFFF" } },
					"alignment": "center"
				}],
				"dataLabels": [{
					"color": { "solid": { "color": "#01B8AA"}},
					"fontSize": 10,
					"fontFamily": "Verdana"
				}],
				"categoryLabels": [{
					"show": true,
					"color": { "solid": { "color": "#000000"}},
					"fontSize": 10,
					"fontFamily": "Verdana"
				}],
				"cardTitle": [{
					"color": { "solid": { "color": "#01B8AA"}},
					"fontSize": 10,
					"fontFamily": "Verdana"
				}],
				"card": [{
					"outline": "TopOnly",
					"outlineColor": { "solid": { "color": "#000000"}},
					"outlineWeight": 2,
					"barShow": true,
					"barColor": { "solid": { "color": "#01B8AA"}},
					"barWeight": 2,
					"cardPadding": 20,
					"cardBackground": { "solid": { "color": "#EEEEEE"}}
				}]
			}
		}
	}
}