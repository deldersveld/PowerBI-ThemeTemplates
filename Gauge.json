{
	"name": "GaugeTemplate",
	"visualStyles": {
		"gauge": {
			"*": {
				"general": [{
					"responsive": true,
					"keepLayerOrder": true
				}],
				"background": [{
					"show": true,
					"color": { "solid": { "color": "#FFFFFF"}},
					"transparency": 0
				}],
				"border": [{
					"color": { "solid": { "color": "#808080"}},
					"show": true,
					"radius": 0
				}],
				"title": [{
					"show": true,
					"fontColor": { "solid": { "color": "#000000" } },
					"background": { "solid": { "color": "#FFFFFF" } },
					"alignment": "center"
				}],
				"axis": [{
					"max": 100,
					"target": 101
				}],
				"dataPoint": [{
					"fill": { "solid": { "color": "#01B8AA"}},
					"target": { "solid": { "color": "#FFB8DD"}}
				}],
				"labels": [{
					"show": true,
					"color": { "solid": { "color": "#EEEEEE"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"fontSize": 11,
					"fontFamily": "Courier New"
				}],	
				"target": [{
					"show": true,
					"color": { "solid": { "color": "#EEEEEE"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1,
					"fontSize": 11,
					"fontFamily": "Courier New"
				}],
				"calloutValue": [{
					"show": true,
					"color": { "solid": { "color": "#EEEEEE"}},
					"labelDisplayUnits": "0",
					"labelPrecision": 1
				}]					
			}
		}
	}
}